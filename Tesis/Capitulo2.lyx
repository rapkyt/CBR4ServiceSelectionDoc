\begin_layout Subsection
Análisis de Compatibilidad de Interfaces
\begin_inset CommandInset label
LatexCommand label
name "sec:Compatibilidad-InterfacesOld"

\end_inset


\begin_inset Note Note
status open

\begin_layout Plain Layout
